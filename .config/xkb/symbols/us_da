default xkb_symbols "us_da" {
    include "us"
    
    key <AD11> { [ bracketleft, braceleft, aring,  Aring ] };
    key <AC10> { [ semicolon,   colon,     ae,     AE ] };
    key <AC11> { [ apostrophe,  quotedbl,  oslash, Ooblique ] };
};
